.my-order {
  width: 100vw;
  height: 100vh;
  overflow: auto;
}
.order-list {
  width: 100vw;
  padding: 10px 0;
  position: relative;
  background-color: #fff;
  margin-bottom: 5px;
}
.order-top{
  width: 100vw;
  display: flex;
  justify-content: space-between;
}
.order-pic {
  width: 60vw;
  display: flex;
  align-items: center;
}
.order-pic image {
  width: 45%;
  height: 90px;
}
.order-count {
  line-height: 90px;
  margin-right: 7%;
}
.order-bottom {
  display: flex;
  justify-content: flex-end;
  margin-right: 10%;
}
.del {
  position: absolute;
  width: 20px;
  height: 20px;
  bottom: 10px;
  right: 10px;
}
.del image {
  width: 100%;
  height: 100%;
}

.order-price {
  margin-top: 5px;
}